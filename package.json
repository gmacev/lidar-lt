{
    "name": "lidar-lt",
    "private": true,
    "version": "0.0.0",
    "description": "Interactive web application for exploring Lithuania's LiDAR point cloud data",
    "author": {
        "name": "Giedrius MaceviÄius",
        "email": "gmacev@gmail.com",
        "url": "https://github.com/gmacev/"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gmacev/lidar-lt"
    },
    "keywords": [
        "lidar",
        "point-cloud",
        "potree",
        "lithuania",
        "3d-visualization",
        "geospatial",
        "react",
        "threejs"
    ],
    "engines": {
        "node": ">=20.19.0"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build && npm run sitemap",
        "sitemap": "npx tsx scripts/generate-sitemap.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
        "type-check": "tsc -b --noEmit",
        "check:circular": "dpdm --circular --no-warning --no-tree src/main.tsx",
        "check:unused": "knip",
        "check:all": "npm run type-check && npm run lint && npm run check:circular && npm run check:unused",
        "preview": "vite preview",
        "serve": "npm run build && vite preview"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.18",
        "@tanstack/react-router": "^1.141.0",
        "@types/three": "^0.181.0",
        "@vis.gl/react-maplibre": "^8.1.0",
        "i18next": "^25.7.3",
        "i18next-browser-languagedetector": "^8.2.0",
        "lodash": "^4.17.21",
        "maplibre-gl": "^5.14.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-helmet-async": "^2.0.5",
        "react-i18next": "^16.5.0",
        "tailwindcss": "^4.1.18",
        "three": "^0.182.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@tanstack/router-plugin": "^1.141.0",
        "@types/geojson": "^7946.0.16",
        "@types/lodash": "^4.17.21",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "babel-plugin-react-compiler": "^1.0.0",
        "dpdm": "^3.14.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react-compiler": "^19.1.0-rc.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-no-manual-memo": "^1.0.4",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "knip": "^5.75.2",
        "prettier": "^3.7.4",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "^7.2.4"
    }
}
